{
  "manifest_version": 3,
  "name": "Custom Inspect Tools",
  "version": "1.0",
  "description": "Opens custom inspect tools for webpages",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Show Inspect Tools"
  }
}